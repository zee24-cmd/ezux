{
    "$schema": "https://typedoc.org/schema.json",
    "entryPoints": [
        "packages/ezux/src/index.ts"
    ],
    "tsconfig": "packages/ezux/tsconfig.json",
    "out": "docs/api",
    "json": "docs/api-model.json",
    "theme": "default",
    "name": "ezUX API Reference",
    "exclude": [
        "**/*.spec.ts",
        "**/*.test.ts"
    ],
    "cleanOutputDir": true,
    "hideGenerator": true,
    "categorizeByGroup": true,
    "navigation": {
        "includeGroups": true,
        "includeCategories": true
    },
    "groupOrder": [
        "Core Components",
        "Services",
        "Properties",
        "Events",
        "Methods",
        "Types",
        "Interfaces",
        "*"
    ],
    "validation": {
        "notExported": false,
        "invalidLink": false,
        "notDocumented": false
    },
    "plugin": []
}